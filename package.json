{
  "name": "movielly",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:robertokbr/movielly.git",
  "author": "robertokbr <robertojuniordev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:setup": "npx prisma migrate dev --name init",
    "db:test": "cross-env DATABASE_URL=file:./db/db-test.sqlite yarn db:setup",
    "test:int": "yarn db:test && jest --config ./jest-int.config.js",
    "test:int:watch": "yarn db:test && jest --config ./jest-int.config.js --watch"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.2",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "prisma": "4.12.0",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "4.12.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0"
  }
}
